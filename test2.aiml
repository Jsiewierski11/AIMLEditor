<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <category>
        <pattern>START SESSION 1 *</pattern>
        <template>
            <think>
                <set name="username">star</set>
                <set name="topic">Session 1</set>
            </think>
            Ok. Let's begin our session. How are you doing today <star/>?
            <oob><robot></robot></oob>
        </template>
    </category>
    <topic name="session"><category>
        <pattern>*</pattern>
        <template>
            <think><set name="data"><star/></set></think>
            <condition name="getsetimnet">
                <li value="verypositive">
                    I am happy
                    <oob><robot><options>
                        <option>Yes</option>
                        <option>No</option>
                    </options></robot></oob>
                </li>
                <li value="positive">I am not as happy</li>
            </condition>
        </template>
    </category></topic>
</aiml>